#create a new repository on the command line
echo "# Hello001" >> README.md
git init
git add README.md
git commit -m "first commit"
git branch -M main
git remote add origin git@github.com:Leslie1989X/Hello001.git
git push -u origin main

#push an existing repository from the command line
git remote add origin git@github.com:Leslie1989X/Hello001.git
git branch -M main
git push -u origin main

#https
https://github.com/Leslie1989X/Hello001.git

#SSH
git@github.com:Leslie1989X/Hello001.git
